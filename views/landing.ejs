							<div id="html" class="experience-icons-container mx-auto">
								<i class="fab fa-html5 experience-icons"></i>
								<h5 class="experience-text" >HTML5</h5>
							</div>
						</div>
						<div class="col-6 col-md-4 mb-3">
							<div id="css" class="experience-icons-container mx-auto">
								<i class="fab fa-css3-alt experience-icons"></i>
								<h5 class="experience-text">CSS3</h5>
							</div>
						</div>
						<div class="col-6 col-md-4 mb-3">
							<div id="js" class="experience-icons-container mx-auto">
								<i class="fab fa-js experience-icons"></i>
